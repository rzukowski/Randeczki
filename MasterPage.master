    <script type="text/javascript" src="Scripts/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="Scripts/jquery.nivo.slider.js"></script>
    <script type="text/javascript" src="Scripts/jquery.fancybox.js"></script>


    <link href="StyleSheet.css" rel="stylesheet" type="text/css" />
    <link href="Content/themes/base/jquery.fancybox.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        $(document).ready(function () {
            $(".fancybox")
                .attr('rel', 'gallery')
                .fancybox({
                    beforeShow: function () {
                        /* Add watermark */
                        $('<div class="watermark"></div>')
                            .bind("contextmenu", function (e) {
                                return false; /* Disables right click */
                            })
                            .prependTo($.fancybox.inner);
                    }
                });
        });
    </script>
</head>
<body>
 <div class="container">
    <form id="form1" runat="server">
        <p class="navbar-text pull-right">
              Witaj <asp:Label runat="server" Id="user"></asp:Label>
        | <i class="icon-off"></i> <asp:LinkButton ID="LinkButton1" OnClick="LogOut" Text="Wyloguj" runat="server"></asp:LinkButton>
            </p>
    <div id="Logo">
      <h3 class="muted" id="log">ktoszuka-tenznajdzie.pl</h3><br />
      <div class="navbar">
        <div class="navbar-inner">
        <asp:Menu runat="server" DataSourceID="MapaSerwisu" Orientation="Horizontal"  EnableTheming="True" OnMenuItemClick="MenuGlowne_MenuItemClick" CssClass="container" StaticMenuStyle-CssClass="nav" DynamicEnableDefaultPopOutImage="True" DynamicMenuItemStyle-BorderStyle="NotSet">
            <DynamicHoverStyle CssClass="nav li active" />
            <DynamicMenuItemStyle CssClass="dropdown-menu" />
            <StaticHoverStyle CssClass="nav li:hover" />
            <StaticMenuItemStyle CssClass="navbar nav active" />
        </asp:Menu>
        </div>
      </div>
    </div>
        
  <div class="span4 well" style="width:130px">
    <div id="Menu">
        <span class="pull-right" style="margin-top:-5px;margin-right: -10px;">
