            </a>
        </span>
     <p>
         <asp:Image ID="photo" runat="server"  Width="100px" Height="100px"  CssClass="img-polaroid"/>
     </p> 
      
            <asp:SiteMapDataSource ID="MapaSerwisu" runat="server" ShowStartingNode="False" />

        <span class="badge badge-info">
            <a href="Odwiedzili.aspx" style="color:yellow;">
                <asp:Label runat="server" ID="OdwiedziliNew"></asp:Label>
                <asp:Label runat="server" ID="OdwiedziliTotal"></asp:Label>
            </a> odwiedzin
        </span>
        <br />

       </div>
   </div>
    <div class="span9" style="border:1px solid rgb(229, 229, 229);box-shadow: 1px 2px rgba(0, 0, 0, 0.05);background-color:rgb(245, 245, 245);">
        <div id="Content">
          <div class="span4" style="width:100%;margin-left:0px;margin-top:5px;">
            <asp:ContentPlaceHolder id="ContentPlaceHolder1" runat="server"></asp:ContentPlaceHolder>
          </div>
        </div>
    </div>

    </form>
    <script type="text/javascript">
        $(window).load(function () {
            $('#slider').nivoSlider();
        });
    </script>
  </div>
